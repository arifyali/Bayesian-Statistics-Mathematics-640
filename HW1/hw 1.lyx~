\begin_layout Part*
Exercise 3
\end_layout

\begin_layout Section*
Part A
\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(\theta)=\frac{\Gamma\left(\alpha+\beta\right)}{\Gamma(\alpha)\Gamma(\beta)}\theta^{\alpha-1}\left(1-\theta\right)^{\beta-1}$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $p(y_{i}|\theta)=\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\theta^{r}\left(1-\theta\right)^{y_{i}}$
\end_inset

 
\series medium
Note: Since r is known, I don't consider it as a 
\begin_inset Quotes eld
\end_inset

given
\begin_inset Quotes erd
\end_inset

 parameter
\end_layout

\begin_layout Standard
Since 
\begin_inset Formula $y=\left\{ y_{i},y_{2}\ldots,y_{n}\right\} $
\end_inset

 
\end_layout

\begin_layout Paragraph*
\begin_inset Formula $p(y|\theta)=\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\theta^{r}\left(1-\theta\right)^{y_{i}}=\theta^{rn}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}}*\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(\theta|y)=\frac{p(y|\theta)*p(\theta)}{p(y)}=\frac{\theta^{rn}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}}*\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)*\frac{\Gamma\left(\alpha+\beta\right)}{\Gamma(\alpha)\Gamma(\beta)}\theta^{\alpha-1}\left(1-\theta\right)^{\beta-1}}{\int_{0}^{1}\theta^{rn}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}}*\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)*\frac{\Gamma\left(\alpha+\beta\right)}{\Gamma(\alpha)\Gamma(\beta)}\theta^{\alpha-1}\left(1-\theta\right)^{\beta-1}\delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\left(\sum_{i-1}^{n}y_{i}\right)+\beta-1}*\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)*\frac{\Gamma\left(\alpha+\beta\right)}{\Gamma(\alpha)\Gamma(\beta)}}{\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)*\frac{\Gamma\left(\alpha+\beta\right)}{\Gamma(\alpha)\Gamma(\beta)}\int_{0}^{1}\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}\delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}}{\int_{0}^{1}\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}\delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}}{\frac{\Gamma(rn+\alpha)\Gamma(\sum_{i-1}^{n}y_{i}+\beta)}{\Gamma\left(rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta\right)}\int_{0}^{1}\frac{\Gamma\left(rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta\right)}{\Gamma(rn+\alpha)\Gamma(\sum_{i-1}^{n}y_{i}+\beta)}*\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}\delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
Note:
\begin_inset Formula $\int_{0}^{1}\frac{\Gamma\left(rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta\right)}{\Gamma(rn+\alpha)\Gamma(\sum_{i-1}^{n}y_{i}+\beta)}*\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}\delta\theta=1$
\end_inset

 because this is the PDF of 
\begin_inset Formula $Beta(rn+\alpha,\sum_{i-1}^{n}y_{i}+\beta)$
\end_inset

 
\end_layout

\begin_layout Paragraph*

\series bold
\begin_inset Formula $\frac{\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}}{\frac{\Gamma(rn+\alpha)\Gamma(\sum_{i-1}^{n}y_{i}+\beta)}{\Gamma\left(rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta\right)}*1}=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{\Gamma\left(rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta\right)}{\Gamma(rn+\alpha)\Gamma(\sum_{i-1}^{n}y_{i}+\beta)}*\theta^{rn+\alpha-1}*\left(1-\theta\right)^{\sum_{i-1}^{n}y_{i}+\beta-1}$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\theta|y\sim\sim Beta(rn+\alpha,\sum_{i-1}^{n}y_{i}+\beta)$
\end_inset


\end_layout

\begin_layout Section*
Part B
\end_layout

\begin_layout Subsection*
posterior mean
\end_layout

\begin_layout Paragraph*

\series medium
Since 
\begin_inset Formula $\theta|y\sim Beta(rn+\alpha,\sum_{i-1}^{n}y_{i}+\beta)\equiv Beta(rn+\alpha,n\bar{y}+\beta)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $E(\theta|y)=\frac{rn+\alpha}{rn+\alpha+\sum_{i-1}^{n}y_{i}+\beta}=\frac{rn+\alpha}{rn+\alpha+n\bar{y}+\beta}$
\end_inset


\end_layout

\begin_layout Subsection*
MLE
\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $L(\theta)=\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\theta^{r}\left(1-\theta\right)^{y_{i}}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $log(L(\theta))=log\left(\prod_{i=1}^{n}\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\theta^{r}\left(1-\theta\right)^{y_{i}}\right)=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\sum_{i=1}^{n}log\left(\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\theta^{r}\left(1-\theta\right)^{y_{i}}\right)=\sum_{i=1}^{n}log\left(\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\right)+\sum_{i=1}^{n}log\left(\theta^{r}\right)+\sum_{i=1}^{n}log\left(\left(1-\theta\right)^{y_{i}}\right)=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\sum_{i=1}^{n}log\left(\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\right)+\sum_{i=1}^{n}r*log\left(\theta\right)+\sum_{i=1}^{n}y_{i}log\left(\left(1-\theta\right)\right)=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
note: 
\begin_inset Formula $\sum_{i=1}^{n}y_{i}=n\bar{y}$
\end_inset

 since 
\begin_inset Formula $\bar{y}=\frac{1}{n}\sum_{i=1}^{n}y_{i}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\sum_{i=1}^{n}log\left(\left(\begin{array}{c}
y_{i}+r-1\\
y_{i}
\end{array}\right)\right)+n*r*log\left(\theta\right)+n\bar{y}log\left(\left(1-\theta\right)\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\delta/d\theta(log(L(\theta|y)))=0+n*r*\frac{1}{\theta}+n\bar{y}*(-1)*\frac{1}{1-\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{nr}{\theta}-\frac{n\bar{y}}{1-\theta}=\frac{nr}{\theta}+\frac{n\bar{y}}{\theta-1}=0\implies nr(\theta-1)+n\bar{y}*\theta=\left(nr+n\bar{y}\right)\theta-nr=0\implies\left(nr+n\bar{y}\right)\theta=nr$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\therefore\hat{\theta}=\frac{nr}{nr+n\bar{y}}=\frac{r}{r+\bar{y}}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
To confirm this is a maximum critical point, the second derivative of the
 log likelihood must be taken
\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $\delta^{2}/d\theta(log(L(\theta)))=\delta^{2}/d\theta\left(\frac{nr}{\theta}-\frac{n\bar{y}}{1-\theta}\right)=$
\end_inset


\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $\delta^{2}/d\theta\left(nr*\theta^{-1}-n\bar{y}*\left(1-\theta\right)^{-1}\right)=$
\end_inset


\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $-nr*\theta^{-2}-n\bar{y}*(-1)*(-1)*\left(1-\theta\right)^{-2}=$
\end_inset


\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $-nr*\theta^{-2}-n\bar{y}*\left(1-\theta\right)^{-2}$
\end_inset


\begin_inset Formula $ $
\end_inset


\end_layout

\begin_layout Paragraph

\series medium
By substituting 
\begin_inset Formula $\hat{\theta}$
\end_inset

 into the second derivative of the log likelihood, we get
\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $-nr*\left(\frac{r}{r+\bar{y}}\right)^{-2}-n\bar{y}*\left(1-\left(\frac{r}{r+\bar{y}}\right)\right)^{-2}$
\end_inset


\end_layout

\begin_layout Paragraph

\series medium
\begin_inset Formula $ $
\end_inset

Thus the value is negative, which indicates that 
\begin_inset Formula $\hat{\theta}$
\end_inset

 is a maximum.
\end_layout

\begin_layout Subsection*
weighted average of the MLE and the prior mean
\end_layout

\begin_layout Paragraph*

\series medium
note the prior mean is 
\begin_inset Formula $\frac{\alpha}{\alpha+\beta}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{r}{r+\bar{y}}*\omega+(1-\omega)*\frac{\alpha}{\alpha+\beta}=\frac{rn+\alpha}{rn+\alpha+n\bar{y}+\beta}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\omega=\frac{n(r+\bar{y)}}{rn+\alpha+n\bar{y}+\beta}$
\end_inset

 Note: This was choosen in order to cancel out the MLE's denomintor and
 multiply the numerator by 
\begin_inset Formula $n$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $1-\omega=1-\frac{n(r+\bar{y)}}{rn+\alpha+n\bar{y}+\beta}=\frac{rn+\alpha+n\bar{y}+\beta}{rn+\alpha+n\bar{y}+\beta}-\frac{n(r+\bar{y)}}{rn+\alpha+n\bar{y}+\beta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{rn+\alpha+n\bar{y}+\beta}{rn+\alpha+n\bar{y}+\beta}-\frac{(nr+\bar{ny)}}{rn+\alpha+n\bar{y}+\beta}=\frac{\alpha+\beta}{rn+\alpha+n\bar{y}+\beta}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\therefore\frac{r}{r+\bar{y}}*\omega+(1-\omega)*\frac{\alpha}{\alpha+\beta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{r}{r+\bar{y}}*\frac{n(r+\bar{y)}}{rn+\alpha+n\bar{y}+\beta}+\frac{\alpha+\beta}{rn+\alpha+n\bar{y}+\beta}*\frac{\alpha}{\alpha+\beta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $r*\frac{n}{rn+\alpha+n\bar{y}+\beta}+\frac{1}{rn+\alpha+n\bar{y}+\beta}*\alpha=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{rn+\alpha}{rn+\alpha+n\bar{y}+\beta}$
\end_inset


\end_layout

\begin_layout Section*
Part C
\end_layout

\begin_layout Standard
The following is the R code used.
 
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

r = 5 
\end_layout

\begin_layout Plain Layout

y = c(7,10,5,8,6,12,6,9,7,5) 
\end_layout

\begin_layout Plain Layout

alpha = c(1,.5,5,1,5) 
\end_layout

\begin_layout Plain Layout

beta = c(1,.5,5,5,1) 
\end_layout

\begin_layout Plain Layout

par(mfrow = c(3,1)) 
\end_layout

\begin_layout Plain Layout

for(i in 1:length(alpha)){   
\end_layout

\begin_layout Plain Layout

	theta = seq(0,1,0.01)   
\end_layout

\begin_layout Plain Layout

	credible_interval = qbeta(c(0.025,0.975), r*length(Y)+alpha[i], sum(Y)+beta[i])
  	
\end_layout

\begin_layout Plain Layout

	posterior_mean = (r*length(Y)+alpha[i])/(r*length(Y)+alpha[i]+sum(Y)+beta[i])
   	
\end_layout

\begin_layout Plain Layout

	plot(theta,  dbeta(theta, r*length(Y)+alpha[i], sum(Y)+beta[i]),      
   
\end_layout

\begin_layout Plain Layout

		type = "l", col = "blue", xlab = expression(theta),         
\end_layout

\begin_layout Plain Layout

		ylab = "Density",        
\end_layout

\begin_layout Plain Layout

		main = paste("alpha = ", alpha[i],"and", "beta = ", beta[i],         
            	
\end_layout

\begin_layout Plain Layout

		"- blue is posterior and red is prior"),        
\end_layout

\begin_layout Plain Layout

		sub = paste("The Credible Interval is", "(",credible_interval[1],",",
                		credible_interval[2], ")",                    
\end_layout

\begin_layout Plain Layout

		"and the posterior mean is", posterior_mean))
\end_layout

\begin_layout Plain Layout

  lines(theta, dbeta(theta, alpha[i], beta[i]), col = "red") 
\end_layout

\begin_layout Plain Layout

	}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
The red line in the following graphs represent the prior distributions of
 
\begin_inset Formula $\theta$
\end_inset

 and the blue line represent the posterior distributions of 
\begin_inset Formula $\theta$
\end_inset

.
 The subtitle contains the 95% credible interval as well as the posterior
 mean for each 
\begin_inset Formula $\alpha$
\end_inset

 and 
\begin_inset Formula $\beta$
\end_inset

 pairing.
