\end_layout

\begin_layout Part*
Exercise 1
\end_layout

\begin_layout Paragraph*

\series medium
Some things to note, identitical twins have the same gender; however fraternal
 twins can have different genders.
 
\end_layout

\begin_layout Paragraph*

\series medium
The sets below, are 1 if possible, 0 otherwise.
 Each case is assumed the have the same weigh in each set.
\end_layout

\begin_layout Paragraph*
\begin_inset Formula $(identical):\left\{ Boy\&Boy|identical,Boy\&Girl|identical,Girl\&Boy|identical,Girl\&Girl|identical\right\} =\left\{ 1,0,0,1\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $P(Boy|identical)=\frac{1}{2}$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $(fraternity):\left\{ Boy\&Boy|fraternity,Boy\&Girl|fraternity,Girl\&Boy|fraternity,Girl\&Girl|fraternity\right\} =\left\{ 1,1,1,1\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula $P(Boy|fraternity)=\frac{1}{4}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $P(identical|Boy)=\frac{P(identical\; twin\cap twin\cap Boy)}{P(Boy)}=\frac{P(Boy|identical)*P(identical)}{P(Boy|identical)*P(identical)+P(Boy|fraternity)}=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1/2*1/300}{1/2*1/300+1/4*1/125}$
\end_inset


\end_layout

\begin_layout Part*
Exercise 2
\end_layout

\begin_layout Section*
Part A
\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(y_{i}|\theta)=\frac{1}{\sqrt{2\pi}\sigma}exp\left\{ -\frac{1}{2\sigma^{2}}\left(y_{i}-\theta\right)^{2}\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(y|\theta)=\prod_{i=1}^{n}p(y_{i}|\theta)=\prod_{i=1}^{n}\frac{1}{\sqrt{2\pi}\sigma}exp\left\{ -\frac{1}{2\sigma^{2}}\left(y_{i}-\theta\right)^{2}\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}exp\left\{ \sum_{i=1}^{n}-\frac{1}{2\sigma^{2}}\left(y_{i}-\theta\right)^{2}\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}exp\left\{ -\frac{1}{2\sigma^{2}}\sum_{i=1}^{n}\left(y_{i}-\theta\right)^{2}\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\bar{y}=150$
\end_inset

 and 
\begin_inset Formula $\sigma=20$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $P(\theta)=\frac{1}{\sqrt{2\pi}r_{0}}exp\left\{ -\frac{1}{2r_{0}^{2}}\left(\mu_{0}-\theta\right)^{2}\right\} $
\end_inset

 where 
\begin_inset Formula $r_{0}=40$
\end_inset

 and 
\begin_inset Formula $\mu_{0}=180$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(\theta|y)=\frac{\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}exp\left\{ -\frac{1}{2\sigma^{2}}\sum_{i=1}^{n}\left(y_{i}-\theta\right)^{2}\right\} *\frac{1}{\sqrt{2\pi}r_{0}}exp\left\{ -\frac{1}{2r_{0}^{2}}\left(\mu_{0}-\theta\right)^{2}\right\} }{\int_{-\infty}^{\infty}\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}exp\left\{ -\frac{1}{2\sigma^{2}}\sum_{i=1}^{n}\left(y_{i}-\theta\right)^{2}\right\} *\frac{1}{\sqrt{2\pi}r_{0}}exp\left\{ -\frac{1}{2r_{0}^{2}}\left(\mu_{0}-\theta\right)^{2}\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}*\frac{1}{\sqrt{2\pi}r_{0}}exp\left\{ -\frac{1}{2\sigma^{2}}\sum_{i=1}^{n}\left(y_{i}-\theta\right)^{2}-\frac{1}{2r_{0}^{2}}\left(\mu_{0}-\theta\right)^{2}\right\} }{\left(\frac{1}{\sqrt{2\pi}\sigma}\right)^{n}*\frac{1}{\sqrt{2\pi}r_{0}}\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2\sigma^{2}}\sum_{i=1}^{n}\left(y_{i}-\theta\right)^{2}-\frac{1}{2r_{0}^{2}}\left(\mu_{0}-\theta\right)^{2}\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(\sum_{i=1}^{n}\left(y_{i}\right)^{2}-\sum_{i=1}^{n}2\theta y_{i}+n\theta^{2}\right)+\frac{1}{r_{0}^{2}}\left(\mu_{0}^{2}-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(\sum_{i=1}^{n}\left(y_{i}\right)^{2}-\sum_{i=1}^{n}2\theta y_{i}+n\theta^{2}\right)+\frac{1}{r_{0}^{2}}\left(\mu_{0}^{2}-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(-\sum_{i=1}^{n}2\theta y_{i}+n\theta^{2}\right)+\frac{1}{r_{0}^{2}}\left(-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} *exp\left\{ -\frac{1}{2}\left(\frac{\sum_{i=1}^{n}\left(y_{i}\right)^{2}}{\sigma^{2}}+\frac{\mu_{0}^{2}}{r_{0}^{2}}\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(-\sum_{i=1}^{n}2\theta y_{i}+n\theta^{2}\right)+\frac{1}{r_{0}^{2}}\left(-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} \delta\theta*exp\left\{ -\frac{1}{2}\left(\frac{\sum_{i=1}^{n}\left(y_{i}\right)^{2}}{\sigma^{2}}+\frac{\mu_{0}^{2}}{r_{0}^{2}}\right)\right\} }=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
note: let 
\begin_inset Formula $-\sum_{i=1}^{n}2\theta y_{i}=2\theta n\bar{y}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(r_{0}^{2}\left(-2\theta n\bar{y}+n\theta^{2}\right)+\sigma^{2}\left(-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} }{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(r_{0}^{2}\left(-2\theta n\bar{y}+n\theta^{2}\right)+\sigma^{2}\left(-2\theta\mu_{0}+\theta^{2}\right)\right)\right\} }=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(-2\theta n\bar{y}*r_{0}^{2}+n\theta^{2}*r_{0}^{2}-2\theta\mu_{0}*\sigma^{2}+\theta^{2}*\sigma^{2}\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(-2\theta n\bar{y}*r_{0}^{2}+n\theta^{2}*r_{0}^{2}-2\theta\mu_{0}*\sigma^{2}+\theta^{2}*\sigma^{2}\right)\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*\sigma^{2}+n\theta^{2}*r_{0}^{2}-2\theta n\bar{y}*r_{0}^{2}-2\theta\mu_{0}*\sigma^{2}\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*\sigma^{2}+n\theta^{2}*r_{0}^{2}-2\theta n\bar{y}*r_{0}^{2}-2\theta\mu_{0}*\sigma^{2}\right)\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*\left(\sigma^{2}+n*r_{0}^{2}\right)-2\theta\left(n\bar{y}*r_{0}^{2}+\mu_{0}*\sigma^{2}\right)\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*\left(\sigma^{2}+n*r_{0}^{2}\right)-2\theta\left(n\bar{y}*r_{0}^{2}+\mu_{0}*\sigma^{2}\right)\right)\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
let's substitue 
\begin_inset Formula $x=\left(\sigma^{2}+n*r_{0}^{2}\right)$
\end_inset

 and 
\begin_inset Formula $y=\left(n\bar{y}*r_{0}^{2}+\mu_{0}*\sigma^{2}\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*x-2\theta y\right)*\frac{1/x}{1/x}\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}}\left(\theta^{2}*x-2\theta y\right)*\frac{1/x}{1/x}\right\} \delta\theta}=\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta^{2}-2\theta y/x\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta^{2}-2\theta y/x\right)\right\} \delta\theta}=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta^{2}-2\theta y/x\right)\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta^{2}-2\theta y/x\right)\right\} \delta\theta}*\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\left(y/x\right)^{2}\right)\right\} }{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\left(y/x\right)^{2}\right)\right\} }=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta-y/x\right)^{2}\right\} }{\int_{-\infty}^{\infty}exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta-y/x\right)^{2}\right\} \delta\theta}*\frac{\left(\sqrt{2\pi r_{0}^{2}\sigma^{2}*\frac{1}{x}}\right)^{-1}}{\left(\sqrt{2\pi r_{0}^{2}\sigma^{2}*\frac{1}{x}}\right)^{-1}}$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\left(\sqrt{2\pi r_{0}^{2}\sigma^{2}*\frac{1}{x}}\right)^{-1}*exp\left\{ -\frac{1}{2r_{0}^{2}\sigma^{2}*\frac{1}{x}}\left(\theta-y/x\right)^{2}\right\} \sim N(y/x,r_{0}^{2}\sigma^{2}/x)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\theta|y\sim N(y/x,r_{0}^{2}\sigma^{2}x)\equiv N\left(\frac{\left(n\bar{y}*r_{0}^{2}+\mu_{0}*\sigma^{2}\right)}{\left(\sigma^{2}+n*r_{0}^{2}\right)},r_{0}^{2}\sigma^{2}/\left(\sigma^{2}+n*r_{0}^{2}\right)\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
After resubstituting everything back in
\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\theta|y\sim N\left(\frac{n*150*40^{2}+180*20^{2}}{20^{2}+10*40^{2}},\frac{40^{2}20^{2}}{20^{2}+n*40^{2}}\right)$
\end_inset


\end_layout

\begin_layout Section*
Part B
\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\theta|y\sim N\left(\frac{n*150*40^{2}+180*20^{2}}{20^{2}+10*40^{2}},\frac{40^{2}20^{2}}{20^{2}+n*40^{2}}\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
please note: To simplify 
\begin_inset Formula $p(\theta|y)$
\end_inset

, the book's notation will be used, thus: 
\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\theta|y\sim N\left(\mu_{1},r_{1}^{2}\right)$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $p(\tilde{y}|\theta)=\frac{1}{\sqrt{2\pi}\sigma}exp\left\{ -\frac{1}{2\sigma^{2}}\left(\tilde{y}-\theta\right)^{2}\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $p(\tilde{y}|y)=\int_{-\infty}^{\infty}p(\tilde{y}|\theta)*p(\theta|y)\delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma}exp\left\{ -\frac{1}{2\sigma^{2}}\left(\tilde{y}-\theta\right)^{2}\right\} *\frac{1}{\sqrt{2\pi}r_{1}}*exp\left\{ -\frac{1}{2r_{1}^{2}}\left(\theta-\mu_{1}\right)^{2}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\int_{-\infty}^{\infty}\frac{1}{2\pi\sigma r_{1}}exp\left\{ -\frac{1}{2\sigma^{2}}\left(\tilde{y}-\theta\right)^{2}-\frac{1}{2r_{1}^{2}}\left(\theta-\mu_{1}\right)^{2}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(\tilde{y}-\theta\right)^{2}+\frac{1}{r_{1}^{2}}\left(\theta-\mu_{1}\right)^{2}\right)\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\left(\frac{1}{\sigma^{2}}\left(\tilde{y}^{2}-2\widetilde{y}\theta+\theta^{2}\right)+\frac{1}{r_{1}^{2}}\left(\theta^{2}-2\theta\mu_{1}+\mu_{1}^{2}\right)\right)\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{\left(r_{1}^{2}\left(\tilde{y}^{2}-2\widetilde{y}\theta+\theta^{2}\right)+\sigma^{2}\left(\theta^{2}-2\theta\mu_{1}+\mu_{1}^{2}\right)\right)}{\sigma^{2}r_{1}^{2}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{\left(r_{1}^{2}\tilde{y}^{2}-r_{1}^{2}2\widetilde{y}\theta+r_{1}^{2}\theta^{2}+\sigma^{2}\theta^{2}-\sigma^{2}2\theta\mu_{1}+\sigma^{2}\mu_{1}^{2}\right)}{\sigma^{2}r_{1}^{2}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\theta^{2}+\sigma^{2}\theta^{2}-r_{1}^{2}2\widetilde{y}\theta-\sigma^{2}2\theta\mu_{1}+r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\theta^{2}+\sigma^{2}\theta^{2}-r_{1}^{2}2\widetilde{y}\theta-\sigma^{2}2\theta\mu_{1}}{\sigma^{2}r_{1}^{2}}\right\} *exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $ $
\end_inset


\series default

\begin_inset Formula $\frac{1}{\sqrt{2\pi}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}\right\} \int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{\left(r_{1}^{2}+\sigma^{2}\right)\theta^{2}-2\theta\left(r_{1}^{2}\widetilde{y}+\sigma^{2}\mu_{1}\right)}{\sigma^{2}r_{1}^{2}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
As this stage, addition substitution is needed
\begin_inset Formula $\therefore x=r_{1}^{2}+\sigma^{2}\; and\; y=r_{1}^{2}\widetilde{y}+\sigma^{2}\mu_{1}$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}\right\} \int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{x\theta^{2}-2\theta y}{\sigma^{2}r_{1}^{2}}*\frac{x^{-1}}{x^{-1}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}\right\} *exp\left\{ \frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{\theta^{2}-2\theta yx^{-1}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} *exp\left\{ -\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}+\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}}exp\left\{ -\frac{1}{2}\frac{\theta^{2}-2\theta yx^{-1}+\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $x^{-1}\frac{1}{\sqrt{2\pi}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}+\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}*x^{-1}}exp\left\{ -\frac{1}{2}\frac{\left(\theta-yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \delta\theta=$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
The integral
\begin_inset Formula $\int_{-\infty}^{\infty}\frac{1}{\sqrt{2\pi}\sigma r_{1}*x^{-1}}exp\left\{ -\frac{1}{2}\frac{\left(\theta-yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} \delta\theta$
\end_inset

 can be recognized as a PDF
\begin_inset Formula $\sim N(yx^{-1},\sigma^{2}r_{1}^{2}\sqrt{x^{-1}})\therefore$
\end_inset

the value is 1
\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}+\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} *1=$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}+\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2}\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}+\frac{1}{2}*\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2}\left[\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}-\frac{\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}x^{-1}}\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2}\left[\frac{r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}}{\sigma^{2}r_{1}^{2}}-\frac{x*\left(yx^{-1}\right)^{2}}{\sigma^{2}r_{1}^{2}}\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi x}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}}\left[r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}-x^{-1}*y^{2}\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*

\series medium
At this point, I'm replacing the variables 
\begin_inset Formula $x$
\end_inset

 and 
\begin_inset Formula $y$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}}\left[r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}-\left(r_{1}^{2}+\sigma^{2}\right)^{-1}*\left(r_{1}^{2}\widetilde{y}+\sigma^{2}\mu_{1}\right)^{2}\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}}\left[r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}-\left(r_{1}^{2}+\sigma^{2}\right)^{-1}*\left(\left(r_{1}^{2}\widetilde{y}\right)^{2}+2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}+\left(\sigma^{2}\mu_{1}\right)^{2}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}}\left[\left(r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}\right)*\frac{r_{1}^{2}+\sigma^{2}}{r_{1}^{2}+\sigma^{2}}-\left(r_{1}^{2}+\sigma^{2}\right)^{-1}*\left(\left(r_{1}^{2}\widetilde{y}\right)^{2}+2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}+\left(\sigma^{2}\mu_{1}\right)^{2}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}*\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}\right)*\left(r_{1}^{2}+\sigma^{2}\right)-\left(\left(r_{1}^{2}\widetilde{y}\right)^{2}+2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}+\left(\sigma^{2}\mu_{1}\right)^{2}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}*\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(r_{1}^{2}\tilde{y}^{2}+\sigma^{2}\mu_{1}^{2}\right)*\left(r_{1}^{2}+\sigma^{2}\right)-\left(\left(r_{1}^{2}\widetilde{y}\right)^{2}+2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}+\left(\sigma^{2}\mu_{1}\right)^{2}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}*\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(r_{1}^{2}\tilde{y}^{2}*r_{1}^{2}+\sigma^{2}\mu_{1}^{2}*r_{1}^{2}+r_{1}^{2}\tilde{y}^{2}*\sigma^{2}+\sigma^{2}\mu_{1}^{2}*\sigma^{2}\right)-\left(\left(r_{1}^{2}\widetilde{y}\right)^{2}+2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}+\left(\sigma^{2}\mu_{1}\right)^{2}\right)\right]\right\} $
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $=\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}*\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(\sigma^{2}\mu_{1}^{2}*r_{1}^{2}+r_{1}^{2}\tilde{y}^{2}*\sigma^{2}\right)-\left(2*r_{1}^{2}\widetilde{y}*\sigma^{2}\mu_{1}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\sigma^{2}r_{1}^{2}*\left(r_{1}^{2}+\sigma^{2}\right)}\left[\sigma^{2}*r_{1}^{2}\left(\mu_{1}^{2}-2*\widetilde{y}*\mu_{1}+\tilde{y}^{2}\right)\right]\right\} =$
\end_inset


\end_layout

\begin_layout Paragraph*
\begin_inset Formula $\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(\mu_{1}-\tilde{y}\right)^{2}\right]\right\} \equiv\frac{1}{\sqrt{2\pi\left(r_{1}^{2}+\sigma^{2}\right)}}*exp\left\{ -\frac{1}{2\left(r_{1}^{2}+\sigma^{2}\right)}\left[\left(\tilde{y}-\mu_{1}\right)^{2}\right]\right\} $
\end_inset

 
\series medium
(This is the format of a normal distribution's PDF)
\end_layout

\begin_layout Standard
\begin_inset Formula $\therefore\tilde{y}|y\sim N(\mu_{1},r_{1}^{2}+\sigma^{2})\equiv N\left(\frac{n*150*40^{2}+180*20^{2}}{20^{2}+10*40^{2}},\frac{40^{2}20^{2}}{20^{2}+n*40^{2}}+20^{2}\right)$
\end_inset


\end_layout

\begin_layout Section*
Part C
\end_layout

\begin_layout Standard
Please note, for 2C and 2D, I used the equations from the book to derive
 the mean and standard deviation; however, the numbers match with respect
 to my results from 2A and 2B.
\end_layout

